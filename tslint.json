{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": [
      true,
      [
        "src",
        "electron"
      ]
    ],
    "no-var-requires": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "no-submodule-imports": [
      true,
      "src"
    ],
    "member-ordering": false
  },
  "rulesDirectory": []
}